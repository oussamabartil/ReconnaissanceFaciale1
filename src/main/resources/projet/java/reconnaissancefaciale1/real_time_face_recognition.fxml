<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="projet.java.reconnaissancefaciale1.controlleur.RealTimeFaceRecognitionController">
    <ImageView fx:id="cameraView"
               fitWidth="800.0"
               fitHeight="600.0"
               layoutX="0.0"
               layoutY="0.0"
               preserveRatio="true" />
    <Label fx:id="recognitionLabel"
           layoutX="10.0"
           layoutY="620.0"
           prefHeight="100.0"
           prefWidth="780.0"
           style="-fx-font-size: 16px; -fx-text-fill: white; -fx-background-color: rgba(0, 0, 0, 0.7); -fx-padding: 5px;" />
</AnchorPane>
